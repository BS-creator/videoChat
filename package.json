{
  "name": "video-chat",
  "version": "1.0.0",
  "description": "",
  "main": "backend/build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cert-build": "openssl req -x509 -newkey rsa:2048 -keyout sslcert/key.pem -out sslcert/cert.pem -days 365 && openssl rsa -in sslcert/key.pem -out sslcert/newkey.pem && mv sslcert/newkey.pem sslcert/key.pem",
    "frontend-build": "NODE_ENV='production' webpack && go build ./frontend/server",
    "frontend-build-dev": "NODE_ENV='development' webpack && go build ./frontend/server",
    "frontend-watch": "go build ./frontend/server && NODE_ENV='development' webpack --watch",
    "frontend-dev": "webpack-dev-server --history-api-fallback --config webpack.config.hot.js",
    "frontend-start": "./server -addr=127.0.0.1:8080 -dir=./frontend/dist",
    "frontend": "npm run frontend-build && npm run frontend-start",
    "backend-dev": "babel-node ./backend/src/index.js",
    "backend-build": "babel backend/src --out-dir backend/build",
    "backend-start": "node ./backend/build/index.js",
    "backend": "npm run backend-build && npm run backend-start",
	"start": "concurrently \"npm run frontend\" \"npm run backend\""
  },
